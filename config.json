//WARP解锁流媒体V2ray/Xray配置文件
{
    "log": {
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 2053,
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password":""//输入密码
                    }
                ],
                "fallbacks": [
                    {
                        "dest": 80
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/usr/local/etc/xray/certificate.crt",
                            "keyFile": "/usr/local/etc/xray/private.key"
                        }
                    ]
                }
            }
        }
    ],
  "outbounds": [
    {
      "tag":"INTERNET_OUT",
      "protocol": "freedom"
    },
    {
      "tag":"WARP_OUT",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4"
      },
      "sendThrough": "172.16.0.2"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "outboundTag": "WARP_OUT",
        "domain": ["geosite:netflix"]
      },
      {
        "type": "field",
        "outboundTag": "INTERNET_OUT",
        "network": "udp,tcp"
      }
    ]
  },
  "dns": {
    "servers": [
      "1.1.1.1",
      "1.0.0.1",
      {
        "address": "1.0.0.1",
        "port": 53,
        "domains": ["geosite:netflix"]
      }
]
}
}
